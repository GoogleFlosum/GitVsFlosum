<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Out_os_Service_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND (
   Out_of_Service__c  = TRUE,
  ISBLANK( OOSDate__c )
)</errorConditionFormula>
    <errorDisplayField>OOSDate__c</errorDisplayField>
    <errorMessage>Please Enter the out of Service date</errorMessage>
</ValidationRule>
